{
  "name": "react-vite-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean-all": "rm -rf node_modules yarn.lock dist",
    "clear-dist": "rm -rf dist",
    ">start================================================<": "",
    "dev": "yarn type-check && vite --host",
    "preview": "vite preview --port 4173",
    ">build================================================<": "",
    "build": "yarn clear-dist && tsc && vite build",
    ">check================================================<": "",
    "type-check": "tsc --noEmit",
    "lint-scss": "npx stylelint src/**/*.{css,sass,scss}",
    "lint": "eslint . --ext ts,tsx --fix --ignore-path .gitignore --report-unused-disable-directives --max-warnings 0",
    ">test=================================================<": "",
    "test:unit": "jest --watch -i units/main.spec.ts -t",
    "test:unit-coverage": "rimraf coverage && yarn test:unit --coverage"
  },
  "dependencies": {
    "classnames": "2.3.2",
    "i18next": "23.7.6",
    "primeicons": "6.0.1",
    "react": "18.2.0",
    "react-cookie": "6.1.1",
    "react-dom": "18.2.0",
    "react-i18next": "13.5.0",
    "react-router-dom": "6.20.0"
  },
  "devDependencies": {
    "@types/node": "20.10.0",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "@vitejs/plugin-react": "4.2.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.53.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.4",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "sass": "1.69.5",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "vite": "5.0.0",
    "vite-plugin-eslint": "1.8.1"
  }
}
